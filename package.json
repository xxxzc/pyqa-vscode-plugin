{
	"name": "pyqa-vscode",
	"displayName": "pyqa-vscode",
	"description": "a python complete helper",
	"version": "0.0.1",
	"publisher": "zicong xie",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
		  "supported": true
		}
	},
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pyqa-vscode.feedback",
				"title": "pyqa:feedback"
			},
			{
				"command": "pyqa-vscode.complete",
				"title": "pyqa:complete"
			}
		],
		"keybindings": {
			"command": "pyqa-vscode.complete",
			"key": "alt+/",
			"mac": "cmd+/",
			"when": "editorTextFocus"
		},
		"configuration": {
			"type": "object",
			"title": "pyqa-vscode",
			"properties": {
				"pyqaPlugin.server": {
					"type": "string",
					"default": "http://localhost:5000",
					"description": "pyqa server"
				},
				"pyqaPlugin.uuid": {
					"type": "string",
					"default": "guest",
					"description": "your uuid"
				},
				"pyqaPlugin.autoSuggestDelay": {
					"type": "integer",
					"default": 500,
					"description": "delay(ms, >=400ms) after cursor stop to trigger autosuggest, 0 to disable autosuggest"
				},
				"pyqaPlugin.triggers": {
					"type": "array",
					"default": ["?", "!"],
					"description": "trigger character list"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"node-fetch": "2"
	}
}
